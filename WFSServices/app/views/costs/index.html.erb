<p id="notice"><%= notice %></p>

<h1>Costs</h1>

<table>
  <thead>
    <tr>
      <th>Unit cost</th>
      <th>Unit rate</th>
      <th>Avg los</th>
      <th>Rev los</th>
      <th>Staff</th>
      <th>Frequency</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @costs.each do |cost| %>
      <tr>
        <td><%= cost.unit_cost %></td>
        <td><%= cost.unit_rate %></td>
        <td><%= cost.avg_los %></td>
        <td><%= cost.rev_los %></td>
        <td><%= cost.staff %></td>
        <td><%= cost.frequency %></td>
        <td><%= link_to 'Show', cost %></td>
        <td><%= link_to 'Edit', edit_cost_path(cost) %></td>
        <td><%= link_to 'Destroy', cost, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cost', new_cost_path %>
